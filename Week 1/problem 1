#include<iostream>
using namespace std;
int main(){
    int n,k;
    cout<<"Enter size of array and key element:";
    cin>>n>>k;
    int arr[n];
    for(int i=0;i<n;i++)
        cin>>arr[i];
    for(int i=0;i<n;i++){
        if(arr[i]==k){
            cout<<"Present"<<endl<<"Comparisons:"<<i+1<<endl;
            return 0;
        }
    }
    cout<<"Not Present";
    return 0;
}
